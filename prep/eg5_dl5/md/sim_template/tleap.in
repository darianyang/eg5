source leaprc.gaff2
source leaprc.protein.ff19SB
source leaprc.water.opc
loadamberprep ../../../adp/carlson/adp.prep
loadamberparams ../../../adp/carlson/frcmod.phos
loadamberparams frcmod.ionslm_1264_opc
pdb = loadpdb PDB_TEMP_leap.pdb
charge pdb
#check pdb
solvateoct pdb OPCBOX 20.0
addions2 pdb Na+ 0
addions2 pdb Cl- 0
addions2 pdb Na+ 114
addions2 pdb Cl- 114
saveamberparm pdb PDB_TEMP_solv.prmtop PDB_TEMP_solv.inpcrd
savepdb pdb PDB_TEMP_solv.pdb
quit
